{
  "name": "demosemver",
  "version": "1.1.1-semantically-released",
  "description": "demo sem ver",
  "main": "index.js",
  "scripts": {
    "test": "node test/syncTest.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AAMLLe/SecondSemVer.git"
  },
  "keywords": [
    "demo",
    "semantic",
    "version"
  ],
  "release": {
  "branch": "master",
  "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "docs", "scope":"README", "release": "patch"},
        {"type": "refactor", "release": "patch"},
        {"type": "style", "release": "patch"}
      ],
      "parserOpts": {
        "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES", "BREAKING"]
      }
    }
},
  "author": "AL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AAMLLe/SecondSemVer/issues"
  },
  "homepage": "https://github.com/AAMLLe/SecondSemVer#readme",
  "dependencies": {
    "colors": "^1.1.2"
  },
  "devDependencies": {
    "semantic-release": "^8.2.0"
  }
}
